<!DOCTYPE html>
<html>
  <head>
    <title>SmallBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
    <nav class="the-nav nav navbar-default navbar-fixed-top">
      <div class="container">


      
        <div class="navbar-header">
          <strong class="navbar-brand"> <%= link_to "CS195 Blog","/"%></strong>
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#header-items" >
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
         
          </button>
        </div>




          <div class="collapse navbar-collapse" id="header-items">

          <ul class="nav navbar-nav navbar">

            <li class="nav-item">
              <%= link_to "About", "/about/"%>
            </li>
            <li class="nav-item">
              <%= link_to "Help", "/help/"%>
            </li>
        
          </ul>
          
          <ul class="nav navbar-nav navbar-right">
            <li class="nav-item">
              <%= link_to "Articles", "/articles/" %>
            </li>
     
            <li>
              <%= link_to "Create Post", "/articles/new/"%>
            </li>

          </ul>

         </div>


      </div> 
    </nav>

    </div>







      <div class="container site-content"> 
        <%= yield %> 
      </div>
  </body>

</html>
